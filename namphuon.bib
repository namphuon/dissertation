@Article{Sunagawa2013,
  Title                    = {{Metagenomic species profiling using universal phylogenetic marker genes}},
  Author                   = {Sunagawa, Shinichi and Mende, Daniel R and Zeller, Georg and Izquierdo-Carrasco, Fernando and Berger, Simon A and Kultima, Jens Roat and Coelho, Luis Pedro and Arumugam, Manimozhiyan and Tap, Julien and Nielsen, H B and Rasmussen, Simon and Brunak, S and Pedersen, Oluf and Guarner, Francisco and de Vos, Willem M and Wang, Jun and Li, Junhua and Dor\'{e}, Jo\"{e}l and Ehrlich, S Dusko and Stamatakis, Alexandros and Bork, Peer},
