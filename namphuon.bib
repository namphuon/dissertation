@Article{Miller2011,
  Title                    = {{EMIRGE: Reconstruction of full length ribosomal genes from microbial community short read sequencing data}},
  Author                   = {Miller, Christopher S and Baker, Brett J and Thomas, Brian C and Singer, Steven W and Banfield, Jillian F},
  Journal                  = {Genome biology},
  Year                     = {2011},

  Month                    = may,
  Number                   = {5},
  Pages                    = {R44},
  Volume                   = {12},

  Abstract                 = {ABSTRACT: Recovery of ribosomal small subunit genes by assembly of short read community DNA sequence data generally fails, making taxonomic characterization difficult. Here, we solve this problem with a novel iterative method, based on the expectation maximization algorithm, that reconstructs full-length small subunit gene sequences and provides estimates of relative taxon abundances. We apply the method to natural and simulated microbial communities, and correctly recover community structure from known and previously unreported rRNA gene sequences. An implementation of the method is freely available at https://github.com/csmiller/EMIRGE.},
  Annote                   = {Emirge - assembles SSU fragments into full length SSU genes. Can map sequences to full-length reference sequences, i.e., map to strains.},
  Doi                      = {10.1186/gb-2011-12-5-r44},
