@Article{Swenson2010,
  Title                    = {{A simulation study comparing supertree and combined analysis methods using SMIDGen}},
  Author                   = {Swenson, M Shel and Barban\c{c}on, Fran\c{c}ois and Warnow, Tandy and Linder, C Randal},
  Journal                  = {Algorithms for molecular biology : AMB},
  Year                     = {2010},

  Month                    = jan,
  Pages                    = {8},
  Volume                   = {5},

  Abstract                 = {Supertree methods comprise one approach to reconstructing large molecular phylogenies given multi-marker datasets: trees are estimated on each marker and then combined into a tree (the "supertree") on the entire set of taxa. Supertrees can be constructed using various algorithmic techniques, with the most common being matrix representation with parsimony (MRP). When the data allow, the competing approach is a combined analysis (also known as a "supermatrix" or "total evidence" approach) whereby the different sequence data matrices for each of the different subsets of taxa are concatenated into a single supermatrix, and a tree is estimated on that supermatrix.},
  Doi                      = {10.1186/1748-7188-5-8},
  File                     = {:projects/sate9/namphuon/papers/Swenson et al. - 2010 - A simulation study comparing supertree and combined analysis methods using SMIDGen.pdf:pdf},
